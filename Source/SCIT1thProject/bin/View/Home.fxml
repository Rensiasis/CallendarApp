<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="mPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.Controller.HomeController">
   <children>
      <MenuBar prefHeight="38.0" prefWidth="600.0">
        <menus>
          <Menu mnemonicParsing="false" text="프로그램">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#btnExitAction" text="프로그램 종료" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="도움말">
            <items>
              <MenuItem mnemonicParsing="false" onAction="#openHelp" text="about" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <Button fx:id="logIn" layoutX="420.0" layoutY="170.0" mnemonicParsing="false" onAction="#btnLogInAction" prefHeight="30.0" prefWidth="150.0" text="LOG IN" />
