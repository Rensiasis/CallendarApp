      <Label fx:id="lPassword" layoutX="360.0" layoutY="125.0" text="PASSWORD :" />
      <TextField fx:id="tfId" layoutX="450.0" layoutY="70.0" prefHeight="30.0" prefWidth="120.0" promptText="ID 입력" />
      <PasswordField fx:id="pfPw" layoutX="450.0" layoutY="120.0" prefHeight="30.0" prefWidth="120.0" promptText="PASSWORD 입력" />
      <Label fx:id="lblMessage" alignment="CENTER" layoutX="363.0" layoutY="312.0" prefHeight="62.0" prefWidth="221.0" text="" textFill="#e80606" />
      <ImageView fx:id="InsertImage" fitHeight="300.0" fitWidth="400.0" layoutX="32.0" layoutY="66.0" pickOnBounds="true" preserveRatio="true">
      <image>
      <Image url="Image/kechiman.PNG" />
      </image>
      </ImageView>
   </children>
</AnchorPane>
